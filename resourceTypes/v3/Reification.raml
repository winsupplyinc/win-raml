#%RAML 1.0 ResourceType
post?:
  description: Post a <<type>>
  body:
    application/json:
      type: <<type>>
      examples: <<postExample>>
  responses:
    201:
      description: Successfully posted <<type>>
      body:
        application/json:
          type:  object
          examples: <<singletonExample>>
    400:
      body:
        application/json:
          type: object
          example:
            message: Invalid Input
    500:
      body:
        application/json:
          type: object
          example:
            message: Internal Server Error